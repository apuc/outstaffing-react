{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "warn",
    "react/prop-types": 0,
    "no-undef": "warn",
    "react/no-unescaped-entities": 0,
    "no-extra-boolean-cast": "warn",
    "react/react-in-jsx-scope": 0
  }
}
